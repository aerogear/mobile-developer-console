{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "coveralls": "^3.0.2",
    "http-proxy-middleware": "^0.19.0",
    "is-git-url": "^1.0.0",
    "lodash": "^4.17.11",
    "lodash-es": "^4.17.10",
    "moment": "^2.22.2",
    "moment-duration-format": "^2.2.2",
    "moment-precise-range-plugin": "^1.3.0",
    "node-sass-chokidar": "~1.3.3",
    "npm-run-all": "^4.1.3",
    "patternfly": "^3.55.0",
    "patternfly-react": "^2.21.5",
    "qrcode.react": "^0.8.0",
    "react": "^16.6.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.6.0",
    "react-jsonschema-form": "^1.0.6",
    "react-moment": "^0.7.9",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom --transformIgnorePatterns \"node_modules/(?!lodash-es)/\"",
    "coverage": "react-scripts test --env=jsdom --coverage --transformIgnorePatterns \"node_modules/(?!lodash-es)/\"",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar --include-path ./node_modules/patternfly/dist/sass --include-path ./node_modules/bootstrap-sass/assets/stylesheets --include-path ./node_modules/font-awesome-sass/assets/stylesheets ./src/ -o ./src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules/patternfly/dist/sass --include-path ./node_modules/bootstrap-sass/assets/stylesheets --include-path ./node_modules/font-awesome-sass/assets/stylesheets ./src/ -o ./src/ --watch --recursive",
    "lint": "eslint 'src/**/*.js'"
  },
  "homepage": "/",
  "devDependencies": {
    "cors-anywhere": "^0.4.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.6.0",
    "eslint-plugin-patternfly-react": "^0.2.0",
    "eslint-plugin-prettier": "^2.7.0",
    "fetch-mock": "^7.2.2",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "redux-mock-store": "^1.5.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
