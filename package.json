{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "0.19.0",
    "body-parser": "1.19.0",
    "classnames": "2.2.6",
    "coveralls": "3.0.6",
    "express": "4.17.1",
    "express-prometheus-middleware": "0.6.1",
    "fast-deep-equal": "2.0.1",
    "highlight.js": "9.15.9",
    "is-git-url": "1.0.0",
    "kubernetes-client": "8.3.3",
    "lodash": "4.17.15",
    "lodash-es": "4.17.15",
    "moment": "2.24.0",
    "moment-duration-format": "2.3.2",
    "node-forge": "0.8.5",
    "node-sass-chokidar": "1.3.5",
    "npm-run-all": "4.1.5",
    "patternfly": "3.59.3",
    "patternfly-react": "2.36.9",
    "qrcode.react": "0.9.3",
    "react": "16.9.0",
    "react-copy-to-clipboard": "5.0.1",
    "react-debounce-input": "3.2.0",
    "react-dom": "16.9.0",
    "react-jsonschema-form": "1.7.0",
    "react-markdown": "4.1.0",
    "react-moment": "0.9.2",
    "react-redux": "5.1.1",
    "react-router-dom": "5.0.1",
    "react-scripts": "3.0.1",
    "redux": "4.0.4",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0"
  },
  "scripts": {
    "start": "node server/server.js",
    "start-js": "react-scripts start",
    "start:client": "npm-run-all -p watch-css start-js",
    "start:server": "nodemon --watch server/server.js --exec 'node server/server.js'",
    "debug:server": "nodemon --watch server/server.js --exec 'node --inspect server/server.js'",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom --transformIgnorePatterns \"node_modules/(?!lodash-es)/\"",
    "coverage": "react-scripts test --env=jsdom --coverage --maxWorkers 4 --transformIgnorePatterns \"node_modules/(?!lodash-es)/\"",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar --include-path ./node_modules/patternfly/dist/sass --include-path ./node_modules/bootstrap-sass/assets/stylesheets --include-path ./node_modules/font-awesome-sass/assets/stylesheets ./src/ -o ./src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules/patternfly/dist/sass --include-path ./node_modules/bootstrap-sass/assets/stylesheets --include-path ./node_modules/font-awesome-sass/assets/stylesheets ./src/ -o ./src/ --watch --recursive",
    "lint": "eslint 'src/**/*.js' 'server/**/*.js'"
  },
  "homepage": "/",
  "devDependencies": {
    "http-proxy-middleware": "0.19.1",
    "axios-mock-adapter": "1.17.0",
    "cors-anywhere": "0.4.1",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint-plugin-patternfly-react": "0.3.0",
    "eslint-plugin-prettier": "3.1.0",
    "fetch-mock": "7.3.9",
    "moxios": "0.4.0",
    "nodemon": "1.19.1",
    "prettier": "1.18.2",
    "redux-mock-store": "1.5.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
